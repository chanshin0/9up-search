{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "google",
    "plugin:prettier/recommended"
  ],
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "require-jsdoc": "off",
    "no-console": "warn",
    "prettier/prettier": [
      "error",
      { "singleQuote": true, "semi": true, "endOfLine": "auto" }
    ]
  }
}
